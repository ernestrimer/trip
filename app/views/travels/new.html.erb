<h1>Add Trip</h1>
<%= form_with(model: @travel, local: true, class: "ui form" ) do |f| %>
  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name, required: true, placeholder: "name your trip" %>

    <%= f.label :start_date %>
    <%= f.text_field :start_date, required: true, placeholder: "yyyy-mm-dd" %>

    <%= f.label :end_date %>
    <%= f.text_field :end_date, required: true, placeholder: "yyyy-mm-dd" %>
  </div>
  <%= f.submit nil, class: "ui blue button inverted" %>
<% end %>



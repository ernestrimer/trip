<h1>Add Location</h1>
<%= form_with(model: @location, local: true, class: "ui form" ) do |f| %>
  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name, required: true, placeholder: "Location Traveled To" %>

    <%= f.label :days %>
    <%= f.text_field :days, required: true  %>

  </div>
  <%= f.submit nil, class: "ui blue button inverted" %>
<% end %>

